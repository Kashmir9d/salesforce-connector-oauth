<?xml version="1.0" encoding="UTF-8"?>

<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes" xmlns:mule="http://www.mulesoft.org/schema/mule/core" prefix="salesforceoauth" url="http://www.mulesoft.org/schema/mule/salesforceoauth">
  <global-cloud-connector extends="http://www.mulesoft.org/schema/mule/salesforceoauth/abstractSalesforceoauthConnectorGeneric" abstract="true" image="" localId="parentConfig" icon="" description="" caption="Salesforceoauth"></global-cloud-connector>
  <global-cloud-connector extends="http://www.mulesoft.org/schema/mule/salesforceoauth/parentConfig" abstract="false" image="icons/large/salesforceoauth-connector-48x32.png" localId="config" icon="icons/small/salesforceoauth-connector-24x16.png" description="Global Salesforceoauth configuration information." caption="Salesforceoauth">
    <attribute-category description="Salesforceoauth configuration properties." caption="General">
      <group caption="Generic" id="salesforceoauthGenericProperties">
        <name required="true" description="Give a name to this configuration so it can be later referenced by config-ref." caption="Name" name="name"></name>
      </group>
      <group caption="General" id="general">
        <string javaType="java.lang.String" required="true" description="API key." caption="Api Key" name="apiKey"></string>
        <string javaType="java.lang.String" required="true" description="API secret." caption="Api Secret" name="apiSecret"></string>
      </group>
    </attribute-category>
    <attribute-category description="OAuth." caption="Oauth">
      <group caption="Oauth Callback Config" id="oauthCallbackConfigGroup">
        <childElement inplace="true" description="" caption="" name="http://www.mulesoft.org/schema/mule/salesforceoauth/oauth-callback-config"></childElement>
      </group>
      <group caption="Oauth Object Store Config" id="oauthStoreConfigGroup">
        <childElement inplace="true" description="" caption="" name="http://www.mulesoft.org/schema/mule/salesforceoauth/oauth-store-config"></childElement>
      </group>
    </attribute-category>
  </global-cloud-connector>
  <cloud-connector abstract="true" image="icons/large/salesforceoauth-connector-48x32.png" localId="abstractSalesforceoauthConnectorGeneric" icon="icons/small/salesforceoauth-connector-24x16.png" description="Interact with Salesforceoauth." caption="Abstractsalesforceoauthconnectorgeneric">
    <attribute-category description="General." caption="General">
      <group caption="Generic" id="abstractSalesforceoauthConnectorGeneric">
        <globalRef requiredType="http://www.mulesoft.org/schema/mule/salesforceoauth/config" required="true" description="Specify which configuration to use for this invocation." caption="Config Reference" name="config-ref"></globalRef>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector extends="http://www.mulesoft.org/schema/mule/salesforceoauth/abstractSalesforceoauthConnectorGeneric" image="icons/large/salesforceoauth-connector-48x32.png" localId="salesforceoauth-connector" icon="icons/small/salesforceoauth-connector-24x16.png" description="Cloud Connector." caption="Salesforceoauth" aliasId="org.mule.tooling.ui.modules.core.pattern.salesforceoauth">
    <attribute-category description="General." caption="General">
      <group caption="Generic" id="salesforceoauthConnectorGeneric">
        <modeSwitch alwaysCombo="true" description="Operation." caption="Operation" name="Salesforceoauth operations to execute">
          <mode modeLabel="Authorize" modeId="http://www.mulesoft.org/schema/mule/salesforceoauth/authorize"></mode>
          <mode modeLabel="Unauthorize" modeId="http://www.mulesoft.org/schema/mule/salesforceoauth/unauthorize"></mode>
          <mode modeLabel="Retrieve contact" modeId="http://www.mulesoft.org/schema/mule/salesforceoauth/retrieve-contact"></mode>
        </modeSwitch>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector extends="http://www.mulesoft.org/schema/mule/salesforceoauth/abstractSalesforceoauthConnectorGeneric" abstract="true" image="icons/large/salesforceoauth-connector-48x32.png" localId="authorize" icon="icons/small/salesforceoauth-connector-24x16.png" caption="Authorize">
    <attribute-category description="General." caption="General">
      <group caption="General" id="general">
        <string required="false" description="Any value that you wish to be sent with the callback." caption="State" name="state"></string>
        <string required="false" description="The URL defined by the Service Provider to obtain an access token." caption="Access Token Url" name="accessTokenUrl"></string>
        <string required="false" description="The URL defined by the Service Provider where the resource owner will be redirected to grant authorization to the connector." caption="Authorization Url" name="authorizationUrl"></string>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector returnType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;" extends="http://www.mulesoft.org/schema/mule/salesforceoauth/abstractSalesforceoauthConnectorGeneric" abstract="true" image="icons/large/salesforceoauth-connector-48x32.png" localId="retrieve-contact" icon="icons/small/salesforceoauth-connector-24x16.png" caption="Retrieve Contact">
    <attribute-category description="General." caption="General">
      <group caption="General" id="general">
        <string required="false" description="The id of the access token that will be used to authenticate the call." caption="Access Token Id" name="accessTokenId"></string>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector extends="http://www.mulesoft.org/schema/mule/salesforceoauth/abstractSalesforceoauthConnectorGeneric" abstract="true" image="icons/large/salesforceoauth-connector-48x32.png" localId="unauthorize" icon="icons/small/salesforceoauth-connector-24x16.png" caption="Unauthorize">
    <attribute-category description="General." caption="General">
      <group caption="General" id="general">
        <string required="false" description="The id of the access token that will be used to authenticate the call." caption="Access Token Id" name="accessTokenId"></string>
      </group>
    </attribute-category>
  </cloud-connector>
  <nested abstract="true" localId="oauth-callback-config" description="" caption="">
    <string description="The domain name to use when setting up the callback endpoint." caption="Domain" name="domain"></string>
    <string description="The local port to use when setting up the callback endpoint." caption="Local Port" name="localPort"></string>
    <string description="The remote port is the port used to build the external callback URL." caption="Remote Port" name="remotePort"></string>
    <string description="The path to use when setting up the callback endpoint." caption="Path" name="path"></string>
    <boolean fillLine="true" description="Async." caption="Async" name="false"></boolean>
  </nested>
  <nested abstract="true" localId="oauth-store-config" description="Configuration element for storage of access tokens" caption="">
    <string description="Object store reference." caption="Object Store Reference" name="objectStore-ref"></string>
  </nested>
</namespace>